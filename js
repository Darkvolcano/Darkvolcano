        <script>
            let y2="";
            const myfruit={
                fruits:[
                    {namE:"Kiwi",information:["New Zealand","150g","1960"]},
                    {namE:"Orange",information:["China","200g","1987"]},
                    {namE:"Mango",information:["Asia","150g","1740"]}
                ]
            }
            for(let i5 in myfruit.fruits){
                y2 += "<h2>" + myfruit.fruits[i5].namE + "</h2>";
                for(let j in myfruit.fruits[i5].information){
                    y2 += myfruit.fruits[i5].information[j] + "<br>";
                }
            }
            document.getElementById("demo133").innerHTML=y2.toUpperCase();
        </script>
