        <script>
            class shoes{
                constructor(brand){
                    this.shoesname=brand;
                }
                present(){
                    return "My shoes is " + this.shoesname;
                }
            }
            class Model extends shoes{
                constructor(brand,mot){
                    super(brand);
                    this.model=mot;
                }
                show(){
                    return this.present() + " ,it is a " + this.model;
                }
            }
            const myshoes=new Model("Nike","running shoes");
            document.getElementById("demo141").innerHTML=myshoes.show();
        </script>
