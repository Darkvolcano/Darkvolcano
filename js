        <script>
            function function5(){
                let voteable;
                let age1=Number(document.getElementById("age1").value);
                if(isNaN(age1)){
                    voteable="Input is not a number"
                }else{
                    voteable=(age1<18) ? "You are too young":"You are old enough";
                }
                document.getElementById("demo97").innerHTML=voteable + " to vote";
            }
        </script>
