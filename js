            function Game(name2,old,level,length){
                this.gamename=name2;
                this.old=old;
                this.gaminglevel=level;
                this.long=length;
            }
            const myfriend=new Game("Dragon city",11,97,5);
            const myself=new Game("Dragon city",11,115,11);
            myself.game=function(){
                return "I have played " + this.gamename.toLowerCase() + " for " + this.long + " years " + " level " + this.gaminglevel;
            };
            document.getElementById("demo134").innerHTML=myself.game();
        </script>
