        <script>
            class Sport{
                constructor(NAME,YEAR){
                    this.NAME=NAME;
                    this.YEAR=YEAR;
                }
                AGE(){
                    const DATE=new Date()
                    return DATE.getFullYear()-this.YEAR;
                }
            }
            const mySport1=new Sport("football",1888);
            const mySport2=new Sport("basketball",1891);
            document.getElementById("demo125").innerHTML=mySport1.AGE();
            document.getElementById("demo126").innerHTML=mySport1.NAME + " " + mySport1.YEAR;
        </script>
