            list:{
                txt8 += phone8[0] + "<br>";
                txt8 += phone8[1] + "<br>";
                txt8 += phone8[2] + "<br>";
                break list;
                txt8 += phone8[3] + "<br>";
            }
            document.getElementById("demo107").innerHTML=txt8;
        </script>
